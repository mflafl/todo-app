    <div class="col-md-4">
        <div class="panel panel-default">
            <div class="panel-heading">Create notebook</div>
            <div class="panel-body">
                <div class="form-group">
                    {{input class='form-control' type="text" value=model.title}}    
                </div>
                <div class="form-group">
                    <button class="btn btn-default" {{action 'save'}}>Save</button>
                    <button class="btn btn-default" {{action 'cancel'}}>Cancel</button>
                </div>
            </div>
        </div>
    </div>